{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "baseUrl": "src",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictBindCallApply": true    
  },
  "include": [
    "src"
  ]
}